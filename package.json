{
  "name": "vue-single-date-picker",
  "version": "0.5.12",
  "main": "./dist/vue-single-date-picker.common.js",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "homepage": "https://jiayingy.github.io/vue-single-date-picker/",
  "keywords": [
    "vue",
    "datepicker",
    "date-picker",
    "date",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jiayingy/vue-single-date-picker"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "production": "yarn build --target lib --name vue-single-date-picker ./src/CalendarView.vue",
    "demo": "yarn build ./src/demo.js",
    "prepare": "yarn production"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.3.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^6.0.1",
    "sass-loader": "^10.2.0",
    "serialize-javascript": "^2.1.2",
    "style-resources-loader": "^1.2.1",
    "vue": "~2.6.10",
    "vue-template-compiler": "~2.6.10"
  },
  "peerDependencies": {
    "vue": "~2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-unused-components": "warn",
      "no-unused-vars": "warn",
      "quotes": [
        "error",
        "single"
      ]
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
